<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PVZRHTools"
        xmlns:hc="https://handyorg.github.io/handycontrol"
        xmlns:fh="clr-namespace:FastHotKeyForWPF;assembly=FastHotKeyForWPF"
        xmlns:sys="clr-namespace:System;assembly=mscorlib"
        x:Class="PVZRHTools.MainWindow"
        mc:Ignorable="d"
        Height="450" Width="800"
        Icon="./PVZRHTools/icon.ico" WindowStyle="None" Background="#991A1A1A"
        Foreground="#FF282C34" BorderThickness="2,2,2,2" UseLayoutRounding="False"
        ResizeMode="NoResize" d:DataContext="{x:Type local:ModifierViewModel}">
    <Window.DataContext>
        <local:ModifierViewModel />
    </Window.DataContext>
    <Window.Resources>
        <ObjectDataProvider x:Key="KeyCodeEnum" MethodName="GetValues" ObjectType="{x:Type sys:Enum}">
            <ObjectDataProvider.MethodParameters>
                <x:Type TypeName="local:KeyCode" />
            </ObjectDataProvider.MethodParameters>
        </ObjectDataProvider>
    </Window.Resources>
    <Window.BorderBrush>
        <LinearGradientBrush EndPoint="1,1" StartPoint="0,0">
            <GradientStop Color="#7FFF3A3A" Offset="1" />
            <GradientStop Color="#7FFF3535" Offset="0" />
            <GradientStop Color="#7FF4FF31" Offset="0.135" />
            <GradientStop Color="#7FA845FF" Offset="0.857" />
            <GradientStop Color="#7F5BFF3A" Offset="0.274" />
            <GradientStop Color="#7F3E6DFF" Offset="0.709" />
            <GradientStop Color="#FF40E9FF" Offset="0.413" />
            <GradientStop Color="#7F3AC4FF" Offset="0.548" />
        </LinearGradientBrush>
    </Window.BorderBrush>
    <Grid>
        <Grid x:Name="TitleBar" Margin="0,0,0,402" PreviewMouseDown="TitleBar_PreviewMouseDown">
            <Label x:Name="WindowTitle" Content="" HorizontalAlignment="Left" Height="44" VerticalAlignment="Center" Width="748" Foreground="#FF2FA42F" FontSize="30" Margin="48,0,0,0" Style="{DynamicResource LabelBaseStyle}" />
            <Image x:Name="inf75" Margin="0,0,752,0" Source="/PVZRHTools/icon.png" Stretch="Fill" />
        </Grid>
        <TabControl TabStripPlacement="Left" Margin="0,49,0,0" BorderBrush="#FF212125" Background="#80212125" BorderThickness="0,0,0,0">
            <TabItem Header="{DynamicResource Tab0.Header}">
                <Border Background="#FF212125" Margin="0,8,0,0" BorderBrush="#FF2FA42F" BorderThickness="2,2,2,2">
                    <ScrollViewer HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto">
                        <Grid>
                            <StackPanel Orientation="Vertical">
                                <Grid>
                                </Grid>
                                <StackPanel Orientation="Horizontal">
                                    <Label Content="{DynamicResource Tab1.Page1.Header}" HorizontalAlignment="Left" Margin="16,4,0,0" VerticalAlignment="Top" Foreground="#FF2FA42F" Style="{DynamicResource LabelBaseStyle}" />
                                    <Rectangle Height="2" Margin="10,17,0,0" VerticalAlignment="Top" Fill="#FF2FA42F" Width="323" />
                                    <hc:ComboBox hc:TitleElement.MinWidth="120" hc:TitleElement.TitlePlacement="Left" hc:TitleElement.Title="语言/Language/Язык" Height="17" Style="{DynamicResource ComboBoxExtend}" SelectionChanged="ComboBox_SelectionChanged" HorizontalAlignment="Right" Width="255" VerticalAlignment="Top" Margin="10,3,0,0">
                                        <ComboBoxItem Content="简体中文" IsSelected="True" />
                                        <ComboBoxItem Content="English" />
                                        <ComboBoxItem Content="Русский" />
                                    </hc:ComboBox>
                                </StackPanel>
                                <StackPanel Orientation="Horizontal">
                                    <CheckBox x:Name="DeveloperMode" Content="{DynamicResource Tab1.Page1.DevMode}" Height="21" Margin="10,0,0,0" VerticalAlignment="Top" Foreground="White" IsChecked="{Binding DeveloperMode}" BorderThickness="2,2,2,2" HorizontalAlignment="Left" />
                                    <CheckBox x:Name="GloveNoCD" IsChecked="{Binding GloveNoCD}" Content="{DynamicResource Tab1.Page1.GloveNoCD}" Height="21" Margin="10,0,0,0" VerticalAlignment="Top" Foreground="White"  BorderThickness="2,2,2,2" HorizontalAlignment="Left" />
                                    <CheckBox x:Name="HammerNoCD" IsChecked="{Binding HammerNoCD}" Content="{DynamicResource Tab1.Page1.HammerNoCD}" Height="21" Margin="10,0,0,0" VerticalAlignment="Top" Foreground="White"  BorderThickness="2,2,2,2" HorizontalAlignment="Left" />
                                    <CheckBox x:Name="PlantingNoCD" IsChecked="{Binding PlantingNoCD}" Content="{DynamicResource Tab1.Page1.PlantingNoCD}" Height="21" Margin="10,0,0,0" VerticalAlignment="Top" Foreground="White"  BorderThickness="2,2,2,2" HorizontalAlignment="Center" />
                                    <CheckBox x:Name="FreePlanting" IsChecked="{Binding FreePlanting}" Content="{DynamicResource Tab1.Page1.FreePlanting}" Height="21" Margin="10,0,0,0" VerticalAlignment="Top" Foreground="White"  BorderThickness="2,2,2,2" HorizontalAlignment="Left" />
                                    <CheckBox x:Name="UnlockAllFusions" IsChecked="{Binding UnlockAllFusions}" Content="{DynamicResource Tab1.Page1.UnlockAllFusions}" Height="21" Margin="10,0,0,0" VerticalAlignment="Top" Foreground="White"  BorderThickness="2,2,2,2" HorizontalAlignment="Left" />
                                </StackPanel>
                                <StackPanel Orientation="Horizontal" Margin="0,12,0,0">
                                    <CheckBox x:Name="GloveFullCDEnabled" Height="21" Margin="10,0,0,0" VerticalAlignment="Top" Foreground="White" IsChecked="{Binding GloveFullCDEnabled}" BorderThickness="2,2,2,2" HorizontalAlignment="Left" Content="{DynamicResource Tab1.Page1.LockGloveCD}" />

                                    <hc:NumericUpDown IsEnabled="{Binding GloveFullCDEnabled}" DecimalPlaces="1" x:Name="GloveFullCD" HorizontalAlignment="Left" Margin="10,0,0,0" VerticalAlignment="Top" Width="80" Height="20" Value="{Binding GloveFullCD}" Increment="0.1" Minimum="0" SelectionOpacity="0.1" />
                                    <CheckBox x:Name="HammerFullCDEnabled" Content="{DynamicResource Tab1.Page1.LockHammerCD}" Height="21" Margin="10,0,0,0" VerticalAlignment="Top" Foreground="White" IsChecked="{Binding HammerFullCDEnabled}" BorderThickness="2,2,2,2" HorizontalAlignment="Left" />

                                    <hc:NumericUpDown IsEnabled="{Binding HammerFullCDEnabled}" DecimalPlaces="1" x:Name="HammerFullCD" HorizontalAlignment="Center" VerticalAlignment="Top" Width="80" Height="20" Value="{Binding HammerFullCD}" Margin="10,0,0,0" Minimum="0" Increment="0.1" SelectionOpacity="0.2" />
                                    <CheckBox x:Name="ColumnPlanting" Content="{DynamicResource Tab1.Page4.ColumnPlanting}" Height="21" Foreground="White"  BorderThickness="2,2,2,2" IsChecked="{Binding ColumnPlanting}" Margin="10,0,0,0" VerticalAlignment="Top" />
                                    <CheckBox x:Name="SeedRain" Content="{DynamicResource Tab1.Page4.SeedRain}" Height="21" Foreground="White"  BorderThickness="2,2,2,2" IsChecked="{Binding SeedRain}" Margin="10,0,0,0" VerticalAlignment="Top" />
                                    <CheckBox x:Name="ScaredyDream" Content="{DynamicResource Tab1.Page4.ScaredyDream}" Height="21" Foreground="White"  BorderThickness="2,2,2,2" IsChecked="{Binding ScaredyDream}" Margin="10,0,0,0" VerticalAlignment="Top" />
                                </StackPanel>
                                <StackPanel Orientation="Horizontal" Margin="0,5,0,0">
                                    <hc:NumericUpDown hc:TitleElement.TitlePlacement="Left" hc:TitleElement.Title="{DynamicResource Tab1.Page1.NextWaveCD}" hc:TitleElement.TitleWidth="120" DecimalPlaces="1" x:Name="NewZombieUpdateCD" HorizontalAlignment="Left" VerticalAlignment="Top" Width="200" Height="20" Value="{Binding NewZombieUpdateCD}" Margin="10,0,0,0" Minimum="0" SelectionOpacity="0.2" Style="{DynamicResource NumericUpDownExtend}" ToolTip="{DynamicResource Tab1.Page1.NextWaveCD.Tootip}" Maximum="30" />
                                    <hc:NumericUpDown hc:TitleElement.TitlePlacement="Left" hc:TitleElement.Title="{DynamicResource Tab1.Page1.GameSpeed}" hc:TitleElement.TitleWidth="90" DecimalPlaces="3" x:Name="GameSpeed" HorizontalAlignment="Left" Margin="10,0,0,0" VerticalAlignment="Top" Width="167" Height="20" Value="{Binding GameSpeed,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" Increment="0.1" Minimum="0" SelectionOpacity="0.1" Maximum="16" Style="{DynamicResource NumericUpDownExtend}" />
                                    <Button x:Name="GardenTools" Content="{DynamicResource Tab0.GardenTools}" Command="{Binding GardenToolsCommand}" Margin="10,0,0,0" />
                                </StackPanel>
                                <StackPanel Orientation="Horizontal">
                                    <Label Content="{DynamicResource Tab2.Page2.Header}" HorizontalAlignment="Left" Margin="10,0,0,0" VerticalAlignment="Top" Foreground="#FF2FA42F" Style="{DynamicResource LabelBaseStyle}" />

                                    <Rectangle HorizontalAlignment="Left" Height="2" Margin="10,14,0,0" VerticalAlignment="Top" Width="552" Fill="#FF2FA42F" />
                                </StackPanel>
                                <StackPanel Orientation="Horizontal">
                                    <Label Content="{DynamicResource Tab2.Page1.Description}" HorizontalAlignment="Left" Margin="10,0,0,0" VerticalAlignment="Top" Foreground="White" Style="{DynamicResource LabelBaseStyle}" />

                                    <hc:NumericUpDown DecimalPlaces="0" hc:TitleElement.TitlePlacement="Left" hc:TitleElement.Title="{DynamicResource Tab2.Page1.Row}" hc:TitleElement.TitleWidth="35" x:Name="Row" HorizontalAlignment="Left" Margin="10,0,0,0" VerticalAlignment="Top" Width="90" Value="{Binding Row}" Maximum="12" Minimum="0" Style="{DynamicResource NumericUpDownExtend}" />
                                    <hc:NumericUpDown DecimalPlaces="0" hc:TitleElement.TitlePlacement="Left" hc:TitleElement.Title="{DynamicResource Tab2.Page1.Col}" hc:TitleElement.TitleWidth="30" x:Name="Column" HorizontalAlignment="Left" Margin="10,0,0,0"  VerticalAlignment="Top" Width="90" Value="{Binding Col}" Maximum="24" Minimum="0" Style="{DynamicResource NumericUpDownExtend}" />
                                    <hc:NumericUpDown DecimalPlaces="0" hc:TitleElement.TitlePlacement="Left" hc:TitleElement.Title="{DynamicResource Tab2.Page1.RepeatTimes}" hc:TitleElement.TitleWidth="100" x:Name="Times" HorizontalAlignment="Left" Margin="10,0,0,0" VerticalAlignment="Top" Width="224" Value="{Binding Times}" Maximum="50" Minimum="1" Style="{DynamicResource NumericUpDownExtend}" />
                                </StackPanel>
                                <hc:ElementGroup Orientation="Horizontal" Layout="Stack" Width="552" HorizontalAlignment="Left" Style="{DynamicResource ElementGroupSolid}" Margin="10,0,0,0">
                                    <Button x:Name="CreatePlant" Content="{DynamicResource Tab2.Page1.CreatePlant}" HorizontalAlignment="Left" Height="30" VerticalAlignment="Top" Width="68" Command="{Binding CreatePlantCommand}" Style="{DynamicResource ButtonGroupItemHorizontalFirst}" />
                                    <ComboBox x:Name="PlantType" SelectedValuePath="Key" DisplayMemberPath="Value" ItemsSource="{Binding Plants2}" SelectedValue="{Binding PlantType}" Style="{DynamicResource ComboBoxExtend}" Width="150" HorizontalAlignment="Left" VerticalContentAlignment="Center" Margin="-34,0,0,0" VerticalAlignment="Center" Height="30" />
                                    <Button x:Name="CreateCard" Content="{DynamicResource Tab2.Page1.CreateCard}" Width="122" Command="{Binding CreateCardCommand}" Height="30" />
                                    <Button x:Name="SimplePresents" Content="{DynamicResource Tab2.Page1.SimplePresents}" Width="215" Command="{Binding SimplePresentsCommand}" Height="30" />
                                </hc:ElementGroup>
                                <StackPanel Orientation="Horizontal">
                                    <hc:ElementGroup Orientation="Horizontal" Layout="Stack" Width="217" HorizontalAlignment="Left" Margin="10,0,0,0">
                                        <Button x:Name="CreateZombie" Content="{DynamicResource Tab2.Page1.CreateZombie}" HorizontalAlignment="Left" Height="30" VerticalAlignment="Top" Width="68" Command="{Binding CreateZombieCommand}" />
                                        <ComboBox x:Name="ZombieType" HorizontalAlignment="Left" Height="30" VerticalAlignment="Center" Width="150" SelectedValuePath="Key" DisplayMemberPath="Value" ItemsSource="{Binding Zombies}" SelectedValue="{Binding ZombieType}" />
                                    </hc:ElementGroup>
                                    <CheckBox x:Name="IsMindCtrl" Content="{DynamicResource Tab2.Page1.IsMindCtrl}" Height="30" Foreground="White"  BorderThickness="2,2,2,2" IsChecked="{Binding IsMindCtrl}" Style="{DynamicResource CheckBoxBaseStyle}" BorderBrush="#FF3F3F46" Margin="10,0,0,0" VerticalAlignment="Top" />
                                    <hc:ButtonGroup Margin="10,0,0,0">
                                        <Button x:Name="PlantVase" Content="{DynamicResource Tab2.Page1.PlantVase}" Command="{Binding PlantVaseCommand}" />
                                        <Button x:Name="ZombieVase" Content="{DynamicResource Tab2.Page1.ZombieVase}" Command="{Binding ZombieVaseCommand}" />
                                    </hc:ButtonGroup>
                                </StackPanel>
                                <StackPanel Orientation="Horizontal">
                                    <hc:ElementGroup Orientation="Horizontal" Layout="Stack" HorizontalAlignment="Left" Margin="10,0,0,0">
                                        <Button x:Name="CreateItem" Content="{DynamicResource Tab2.Page1.CreateItem}" HorizontalAlignment="Left" Height="30" VerticalAlignment="Top" Width="68" Command="{Binding CreateItemCommand}" />
                                        <ComboBox x:Name="ItemType" HorizontalAlignment="Left" Height="30" VerticalAlignment="Top" Width="150" SelectedValuePath="Key" DisplayMemberPath="Value" SelectedValue="{Binding ItemType}" ItemsSource="{Binding Items, Mode=OneWay}" />

                                    </hc:ElementGroup>
                                    <hc:ButtonGroup Height="30" Width="auto" Margin="10,0,0,0" Style="{DynamicResource ButtonGroupSolid}" HorizontalAlignment="Left">
                                        <Button x:Name="CreatePassiveMateorite" Content="{DynamicResource Tab2.Page1.CreatePassiveMateorite}" HorizontalAlignment="Left" Height="30" VerticalAlignment="Top" Width="auto"  Command="{Binding CreatePassiveMateoriteCommand}" Style="{DynamicResource ButtonGroupItemHorizontalFirst}" />
                                        <Button x:Name="CreateActiveMateorite" Content="{DynamicResource Tab2.Page1.CreateActiveMateorite}" HorizontalAlignment="Left" Height="30" VerticalAlignment="Top" Width="auto" Command="{Binding CreateActiveMateoriteCommand}" Style="{DynamicResource ButtonGroupItemDefault}" />
                                        <Button x:Name="CreateUltimateMateorite" HorizontalAlignment="Left" Height="30" VerticalAlignment="Top" Width="auto" Content="{DynamicResource Tab2.Page1.CreateUltimateMateorite}" Command="{Binding CreateUltimateMateoriteCommand}" Style="{DynamicResource ButtonGroupItemHorizontalLast}" />
                                    </hc:ButtonGroup>
                                    <Button x:Name="AbyssCheat" Content="{DynamicResource Tab0.AbyssCheat}" Height="30" Command="{Binding AbyssCheatCommand}" HorizontalAlignment="Left" Margin="10,0,0,0" VerticalAlignment="Top" />
                                    <Button x:Name="LoadCustomPlantData" Content="{DynamicResource Tab0.LoadCustomPlantData}" Command="{Binding LoadCustomPlantDataCommand}" Height="30" Margin="10,0,0,0" ToolTip="{DynamicResource Tab0.LoadCustomPlantData.Tooltip}"/>

                                </StackPanel>
                                <StackPanel Orientation="Horizontal" Margin="0,10,0,0">
                                    <Button x:Name="ChangeSun" Content="{DynamicResource Tab2.Page2.ChangeSun}" HorizontalAlignment="Left" Height="30" Margin="10,0,0,0" VerticalAlignment="Top" Width="75" Command="{Binding SunCommand}" />
                                    <hc:NumericUpDown DecimalPlaces="0" x:Name="Sun" HorizontalAlignment="Left" VerticalAlignment="Top" Width="94" Height="30" Value="{Binding NewSun, ValidatesOnNotifyDataErrors=True}" Increment="100" Maximum="2147483647" Minimum="0" Margin="10,0,0,0" />
                                    <CheckBox x:Name="LockSun" IsChecked="{Binding LockSun}" Content="{DynamicResource Tab2.Page2.LockSun}" Height="21" VerticalAlignment="Top" Foreground="White"  BorderThickness="2,2,2,2" Margin="10,5,0,0" />
                                    <Button x:Name="ChangeMoney" Content="{DynamicResource Tab2.Page2.ChangeMoney}" HorizontalAlignment="Left" Height="30" VerticalAlignment="Top" Width="75" Command="{Binding MoneyCommand, ValidatesOnNotifyDataErrors=True}" Margin="10,0,0,0" />
                                    <hc:NumericUpDown DecimalPlaces="0" x:Name="Money" HorizontalAlignment="Left"  VerticalAlignment="Top" Width="94" Height="30" Value="{Binding NewMoney}" Increment="100" Maximum="99999" Minimum="0" Margin="10,0,0,0" />
                                    <CheckBox x:Name="LockMoney" IsChecked="{Binding LockMoney}" Content="{DynamicResource Tab2.Page2.LockMoney}" Height="21" VerticalAlignment="Top" Foreground="White"  BorderThickness="2,2,2,2" Margin="10,5,0,0" />
                                </StackPanel>
                                <StackPanel Orientation="Horizontal" Margin="0,10,0,0">
                                    <Button x:Name="NextWave" Content="{DynamicResource Tab2.Page2.NextWave}" HorizontalAlignment="Left" Height="30" VerticalAlignment="Top" Width="167" Command="{Binding NextWaveCommand}" Margin="10,0,0,0" />
                                    <CheckBox x:Name="StopSummon" IsChecked="{Binding StopSummon}" Content="{DynamicResource Tab2.Page2.StopSummon}" Height="21" VerticalAlignment="Top" Foreground="White"  BorderThickness="2,2,2,2" Margin="10,5,0,0" />
                                    <CheckBox x:Name="NoFail" IsChecked="{Binding NoFail}" Content="{DynamicResource Tab2.Page2.NoFail}" Height="21" VerticalAlignment="Top" Foreground="White"  BorderThickness="2,2,2,2" Margin="10,5,0,0" />
                                    <CheckBox x:Name="BuffRefreshNoLimit" IsChecked="{Binding BuffRefreshNoLimit}" Content="{DynamicResource Tab2.Page2.BuffRefreshNoLimit}" Height="21" Foreground="White"  BorderThickness="2,2,2,2" />
                                </StackPanel>
                                <hc:ButtonGroup Height="30" Margin="10,10,0,0">
                                    <Button x:Name="ClearAllPlants" Content="{DynamicResource Tab2.Page2.ClearAllPlants}" Command="{Binding ClearAllPlantsCommand}" Style="{DynamicResource ButtonGroupItemHorizontalFirst}" />
                                    <Button x:Name="KillAllZombies" Content="{DynamicResource Tab2.Page2.KillAllZombies}" Command="{Binding KillAllZombiesCommand}" Style="{DynamicResource ButtonGroupItemDefault}" />
                                    <Button x:Name="ClearIce" Content="{DynamicResource Tab2.Page2.ClearIce}" Command="{Binding ClearIceRoadsCommand}" Style="{DynamicResource ButtonGroupItemDefault}" />
                                    <Button x:Name="MindCtrlAll" Content="{DynamicResource Tab2.Page2.MindCtrlAll}" Command="{Binding MindCtrlCommand}" Style="{DynamicResource ButtonGroupItemDefault}" />
                                    <Button x:Name="ClearAllHoles" Content="{DynamicResource Tab2.Page2.ClearAllHoles}" Command="{Binding ClearAllHolesCommand}" Style="{DynamicResource ButtonGroupItemDefault}" />
                                    <Button x:Name="SetAward" Content="{DynamicResource Tab2.Page2.SetAward}" Command="{Binding SetAwardCommand}" Style="{DynamicResource ButtonGroupItemDefault}" />
                                </hc:ButtonGroup>
                                <StackPanel Orientation="Horizontal" Margin="0,10,0,0">
                                    <Button x:Name="ChangeLevelName" Content="{DynamicResource Tab2.Page2.ChangeLevelName}" HorizontalAlignment="Left" Height="30" VerticalAlignment="Top" Width="106" Command="{Binding LevelNameCommand}" Margin="10,0,0,0" />
                                    <TextBox x:Name="LevelName" HorizontalAlignment="Left" TextWrapping="Wrap" VerticalAlignment="Top" Width="229" Height="20" Text="{Binding NewLevelName}" Margin="10,0,0,0" />
                                </StackPanel>
                                <StackPanel Orientation="Horizontal" Margin="0,10,0,0">
                                    <Button x:Name="ShowText" Content="{DynamicResource Tab2.Page2.ShowText}" HorizontalAlignment="Left" Height="30" VerticalAlignment="Top" Width="106" Command="{Binding ShowingTextCommand}" Margin="10,0,0,0" />
                                    <TextBox x:Name="Text" HorizontalAlignment="Left" TextWrapping="Wrap" VerticalAlignment="Top" Width="229" Height="20" Text="{Binding ShowText}" Margin="10,0,0,0" />
                                </StackPanel>
                                <hc:ButtonGroup Height="30" HorizontalAlignment="Left" Margin="10,10,0,0">
                                    <Button x:Name="StartMower" Content="{DynamicResource Tab2.Page2.StartMower}"  Command="{Binding StartMowerCommand}" />
                                    <Button x:Name="CreateMower" Content="{DynamicResource Tab2.Page2.CreateMower}"  Command="{Binding CreateMowerCommand}" ToolTip="{DynamicResource Tab2.Page2.CreateMower.Tooltip}" />
                                </hc:ButtonGroup>
                                <StackPanel Orientation="Vertical" Margin="0,10,0,0">
                                    <CheckBox x:Name="ConveyBeltModify" IsChecked="{Binding ConveyBeltModify}" Content="{DynamicResource Tab2.Page1.ConveyBeltModify}" Height="21" VerticalAlignment="Top" Foreground="White"  BorderThickness="2,2,2,2" HorizontalAlignment="Left" Width="192" Margin="10,0,0,0" />

                                    <hc:CheckComboBox x:Name="ConveyBeltTypes" local:SelectedItemsExt.SelectedItems="{Binding ConveyBeltTypes, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" ItemsSource="{Binding Plants2}" HorizontalAlignment="Left" Width="644" SelectedValuePath="Key" DisplayMemberPath="Value" IsSynchronizedWithCurrentItem="True" Margin="10,0,0,0" />
                                </StackPanel>
                            </StackPanel>
                        </Grid>
                    </ScrollViewer>
                </Border>
            </TabItem>

            <TabItem Header="{DynamicResource Tab1.Header}">
                <Border Background="#FF212125" Margin="0,8,0,0" BorderBrush="#FF2FA42F" BorderThickness="2,2,2,2">
                    <ScrollViewer VerticalScrollBarVisibility="Auto">
                        <StackPanel>
                            <Expander Header="{DynamicResource Tab1.Page2.Header}" Style="{DynamicResource Expander.Small}" MinWidth="28" MinHeight="28">
                                <Grid>
                                    <CheckBox x:Name="SuperPresent" IsChecked="{Binding SuperPresent}" Content="{DynamicResource Tab1.Page2.SuperPresent}" Height="21" Margin="10,10,574,0" VerticalAlignment="Top" Foreground="White"  BorderThickness="2,2,2,2" HorizontalAlignment="Left" />
                                    <CheckBox x:Name="UltimateRamdomZombie" IsChecked="{Binding UltimateRamdomZombie}" Content="{DynamicResource Tab1.Page2.UltimateRamdomZombie}" Height="21" Margin="95,10,293,0" VerticalAlignment="Top" Foreground="White"  BorderThickness="2,2,2,2" HorizontalAlignment="Left" />
                                    <CheckBox x:Name="PresentFastOpen" IsChecked="{Binding PresentFastOpen}" Content="{DynamicResource Tab1.Page2.PresentFastOpen}" Height="21" Margin="268,10,0,0" VerticalAlignment="Top" Foreground="White"  BorderThickness="2,2,2,2" HorizontalAlignment="Left" />
                                    <ComboBox SelectedValue="{Binding LockPresent}" hc:TitleElement.TitlePlacement="Left" hc:TitleElement.Title="{DynamicResource Tab1.Page2.LockPresent}" hc:TitleElement.TitleWidth="90" x:Name="LockPresent" HorizontalAlignment="Left" Height="26" Margin="10,36,0,0" Width="217" SelectedValuePath="Key" DisplayMemberPath="Value" ItemsSource="{Binding Plants}" Style="{DynamicResource ComboBoxExtend}" VerticalAlignment="Top" />
                                </Grid>
                            </Expander>
                            <Expander Header="{DynamicResource Tab1.Page3.Header}" Style="{DynamicResource Expander.Small}" MinWidth="28" MinHeight="28">
                                <Grid>
                                    <Label Content="{DynamicResource Tab1.Page3.Description}" HorizontalAlignment="Left" Margin="0,2,0,0" VerticalAlignment="Top" Foreground="White" />
                                    <ComboBox hc:TitleElement.TitlePlacement="Left" hc:TitleElement.Title="{DynamicResource Tab1.Page3.ZombieHP}" hc:TitleElement.TitleWidth="115" x:Name="HealthZombieType" HorizontalAlignment="Left" Height="20" Margin="10,32,0,0" VerticalAlignment="Top" Width="236" SelectedValuePath="Key" DisplayMemberPath="Value" ItemsSource="{Binding Zombies, Mode=OneWay}" SelectedValue="{Binding HealthZombieType}" Style="{DynamicResource ComboBoxExtend}" />
                                    <ComboBox hc:TitleElement.TitlePlacement="Left" hc:TitleElement.Title="{DynamicResource Tab1.Page3.1stArmorHP}" hc:TitleElement.TitleWidth="115" x:Name="Health1stType" HorizontalAlignment="Left" Height="20" Margin="10,62,0,0" VerticalAlignment="Top" Width="236" SelectedValuePath="Key" DisplayMemberPath="Value" ItemsSource="{Binding FirstArmor, Mode=OneWay}" SelectedValue="{Binding Health1stType}" Style="{DynamicResource ComboBoxExtend}" />
                                    <ComboBox hc:TitleElement.TitlePlacement="Left" hc:TitleElement.Title="{DynamicResource Tab1.Page3.2ndArmorHP}" hc:TitleElement.TitleWidth="115" x:Name="Health2ndType" HorizontalAlignment="Left" Height="20" Margin="10,93,0,0" VerticalAlignment="Top" Width="236" SelectedValuePath="Key" DisplayMemberPath="Value" ItemsSource="{Binding SecondArmor, Mode=OneWay}" SelectedValue="{Binding Health2ndType}" Style="{DynamicResource ComboBoxExtend}" />
                                    <ComboBox hc:TitleElement.TitlePlacement="Left" hc:TitleElement.Title="{DynamicResource Tab1.Page3.BulletDamage}" hc:TitleElement.TitleWidth="115" x:Name="bulletDamageType" HorizontalAlignment="Left" Height="20" Margin="10,125,0,0" VerticalAlignment="Top" Width="236" SelectedValuePath="Key" DisplayMemberPath="Value" ItemsSource="{Binding Bullets, Mode=OneWay}" SelectedValue="{Binding BulletDamageType}" Style="{DynamicResource ComboBoxExtend}" />
                                    <hc:NumericUpDown DecimalPlaces="0" x:Name="HealthZombieValue" HorizontalAlignment="Left" Margin="251,32,0,0" VerticalAlignment="Top" Width="60" Height="20" Value="{Binding HealthZombieValue}" />
                                    <hc:NumericUpDown DecimalPlaces="0" x:Name="Health1stValue" HorizontalAlignment="Left" Margin="251,62,0,0" VerticalAlignment="Top" Width="60" Height="20" Value="{Binding Health1stValue}" />
                                    <hc:NumericUpDown DecimalPlaces="0" x:Name="Health2ndValue" HorizontalAlignment="Left" Margin="251,93,0,0" VerticalAlignment="Top" Width="60" Height="20" Value="{Binding Health2ndValue}" />
                                    <hc:NumericUpDown DecimalPlaces="0" x:Name="bulletDamageValue" HorizontalAlignment="Left" Margin="251,125,0,0" VerticalAlignment="Top" Width="60" Height="20" Value="{Binding BulletDamageValue}" />
                                    <Button x:Name="hzbutton" Content="{DynamicResource Modify}" Height="30" Margin="0,32,0,0" VerticalAlignment="Top" Width="70" Command="{Binding HealthZombieCommand}" />
                                    <Button x:Name="h1button" Content="{DynamicResource Modify}" Height="30" Margin="0,62,0,0" VerticalAlignment="Top" Width="70" Command="{Binding Health1stCommand}" />
                                    <Button x:Name="h2button" Content="{DynamicResource Modify}" Height="30" Margin="0,93,0,0" VerticalAlignment="Top" Width="70" Command="{Binding Health2ndCommand}" />
